#Daten aufbereitung
titanic_data <- data.table::fread("titanic_data.csv")
titanic_data <-  titanic_data %>% mutate(Age = replace(Age, Age>0 & Age<1, NA))#Statt Na 0?
titanic_data <- titanic_data %>%  transmute(
Survived =  factor(Survived,
levels = c(0,1),
labels = c("Died","Survived")
),
Class = factor(Pclass),
Sex = factor(Sex),
Age = as.integer(Age),
SibSp,
Parch,
Fare = round(Fare,2),
Cabin = substr( gsub("[^a-zA-Z]", "", Cabin), 1, 1),
Embarked = factor(Embarked,
levels = c("C","Q","S"),
labels = c("Cherbourg","Queenstown","Southampton"))
)
titanic_data[titanic_data == ""] <- NA
library(correlation)
CramerV(xtabs(~Survived+Sex+Class, data=titanic_data))
correlation(xtabs(~Survived+Sex+Class, data=titanic_data),method = "auto")
correlation(xtabs(~Survived+Sex+Class, data=titanic_data),method = auto)
correlation(xtabs(~Survived+Sex+Class, data=titanic_data),method = "auto")
correlation(subset(titanic_data, select = c(Survived,Sex,Class)),method = "auto")
#Relativ
ggplot(titanic_data, aes(x= Survived, group=Sex)) +
geom_bar(aes(y = ..prop.., fill = factor(..x..)), stat="count") +
geom_text(aes( label = scales::percent(..prop..),
y= ..prop.. ), stat= "count", vjust = -.5) +
labs(y = "Percent", fill="Survived?",title = "Survival vs Sex") +
facet_grid(~Sex) +
scale_y_continuous(labels = scales::percent)+
guides(fill="none")
